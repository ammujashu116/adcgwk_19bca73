public class RowSum
{
public static void main(String[]args)
{
int arr[][]={1,3,4,5,6},{4,5,6,7,8},{9,6,3,1,6}};
int sum=0;
System.out.println("the given array is:");
for(int i=0;i<3;i++)
{
for(int j=0;j<5;j++)
{
System.out.print(arr[i][j]+"  ");
}
System.out.println();
}
System.out.println("sum of each row are:");
for(int i=0;i<3;i++)
{
for(int j=0;j<5;j++)
{
sum=sum+arr[i][j]:
}
System.out.print()
}
}
}
