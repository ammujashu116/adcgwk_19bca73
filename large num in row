public class LargeNumInRow
{
public static void main(Stringargs[])
{
int i,j,larg;
int arr[][]={}1,2,3,4,5},{8,6,5,1,3},{2,5,9,6,3}};
System.out.println("given array is");
for(i=0;i<3;i++)
{
for(j=0;j<5;j++)
{
System.out.print(arr[i[][j]+"  ");
}
System.out.println();
}
System.out.println("largest element in each row:");
for(i=0;i<3;i++)
{
larg=arr[i][0];
for(j=0;j<5;j++)
{
if(arr)[i][j]>larg)
{
larg=arr[i][j];
}
}
System.out.println(larg);
}
}
}
