import java.util.*;

class gbc

{

public static void main(String args[])

{

Scanner in=new Scanner(System.in);


 

int tc;

    tc=in.nextInt();

    while(tc-->0)

    {

        int w;

        w=in.nextInt();

        if(w%2==0&&w<=100000)

        {

            if(w!=2)

            System.out.println("Deepa");

            else

            System.out.println("Arjit");

        }

    }

}

}
